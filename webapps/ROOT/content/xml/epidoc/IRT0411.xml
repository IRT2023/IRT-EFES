               <lb n="9"/><w lemma="curia">Curiae</w> <w lemma="tres">tres</w> <w lemma="Plotina">Plotina</w>
               <lb n="10"/><w lemma="et">et</w> <w lemma="Nervius">Neruia</w> <w lemma="et">et</w>  <w lemma="Matidia">Mati<lb n="11" break="no"/>dia</w> <w lemma="pono">posuerunt</w>
